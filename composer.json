{
    "name": "jue/swoole",
    "description": "High concurrent multi-process business processing SwooleProcess",
    "type": "project",
    "authors": [
        {
            "name": "马斯珣",
            "email": "masixun71@foxmail.com"
        }
    ],
    "require": {
        "symfony/console": "@stable",
        "monolog/monolog": "^1.23"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.phpcomposer.com"
        },
        {
            "packagist": false
        }
    ],
    "autoload": {
        "psr-4": {
            "Jue\\Swoole\\": "src/"
        }
    },
    "config": {
        "secure-http": false,
        "preferred-install": {
            "*": "dist"
        }
    }
}
